fun main() >> i32 {
    i32 a = 250;
    i32 b = 50;
    i32 sum = a + b;
    @printf("%d + %d = (expected 300):%d\n", a, b, sum);
    i32 dif = a - b;
    @printf("%d - %d = (expected 200):%d\n", a, b, dif);
    i32 mul = a * b;
    @printf("%d * %d = (expected 12500):%d\n", a, b, mul);
    i32 div = a / b;
    @printf("%d / %d = (expected 5):%d\n", a, b, div);
    i32 br_ = (a+50) / b;
    @printf("(%d+50) / %d = (expected 6):%d\n", a, b, br_);
    a = a + a;
    @printf("250 + 250 = (expected 500):%d\n", a);
    @printf("2 * 2 = %d", 2*2);
    f32 f1 = 3.14;
    f32 f2 = 14.88;
    f64 f3 = f1 * f2;
    @printf("%f + %f = (expected 18.020000):%f\n", f1, f2, f1+f2);
    @printf("%f * %f = (expected 46.723202)%f\n", f1, f2, f3);
    f16 f16_ = 1.2;
    f64 f64_ = 2.1;
    @printf("f64 + f16 test (expected 3.3):%.1f\n", f64_+f16_);
    f64_ = f64_ + f1 + f16_ + 0.0 + 1.1 + 2.222222222;
    
    ?? Modulo tests
    i32 mod_a = 2;
    @printf("(expected 0):%d\n", mod_a % 2);
    i32 mod_b = 44;
    @printf("(expected 2):%d\n", mod_b % 3);

    u16 mod_c = 2;
    @printf("(expected 0):%d\n", mod_c % 2);
    u16 mod_d = 44;
    @printf("(expected 2):%i\n", mod_d % 3);

    i64 mod_e = 2;
    @printf("(expected 0):%i\n", mod_e % 2);
    i64 mod_f = 44;
    @printf("(expected 2):%d\n", mod_f % 3);

    f32 flo = 2.001222;
    @printf("(expected 2.001222):%f\n", flo % 2.011);

    i32 mod_g = 41;
    @printf("(expected 1): %i", mod_g % -2); ?? Why would you do that?
    
    return 0;
}
