fun classify(a: i32) >> i32 {
    i32 result = 0;

    match a + 1 {
        0 {
            result = -1;
        }
        1,2,3 {
            result = 3;
        }
        99 {
            result = 99;
        }
    }

    return result;
}

fun main() >> i32 {
    @printf("(expected -1):%d\n", classify(-1));
    @printf("(expected 3):%d\n", classify(1));
    @printf("(expected 3):%d\n", classify(2));
    @printf("(expected 99):%d\n", classify(98));
    @printf("(expected 0):%d\n", classify(50));
    return 0;
}
