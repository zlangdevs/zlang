fun main() >> i32 {
    i32 simple  = 2;
    i32 neg     = -2;
    i32 pos     = +2;
    i32 exp_pos = 2e2;
    f32 exp_neg = 2e-2;
    f32 exp_zer = 2e0;
    f32 neg_exp = -2e-2;
    f32 neg_var = -neg_exp;
    
    @printf("(expected 2):%d\n", simple);
    @printf("(expected -2):%d\n", neg);
    @printf("(expected 2):%d\n", pos);
    @printf("(expected 200):%d\n", exp_pos);
    @printf("(expected 0.020000):%f\n", exp_neg);
    @printf("(expected 2.000000)%f\n", exp_zer);
    @printf("(expected -0.020000)%f\n", neg_exp);
    @printf("(expected 0.020000)%f\n", neg_var);
    
    return 0;
}